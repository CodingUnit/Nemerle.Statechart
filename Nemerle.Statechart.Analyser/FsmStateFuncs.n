    public TraverseTransitions(this node : GeneralTree[FsmState]) : void
    {
      def paths = TransitionPaths(node);
      node.Update(x => x.ChangePaths(paths.TraverseTransitions(true)));
      //UpdateTransitions(trans).ChangePaths(paths)
    }
    
   



  flags testing auto_initial;
  //flags debug;

  name "join transition";

  state A
  {
    a => B;
  }

  state_conc B
  {
    @(C, F): a => A;

    state C
    {
      b => E;
    }

    state E
    {
      b => C;
    }

  } and
  {

    state D
    {
      a => F;
    }

    state F
    {
      b => D;
    }
  }

  
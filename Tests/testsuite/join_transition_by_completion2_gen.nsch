

  flags testing auto_initial;
  //flags debug;

  name "join transition with completion event";

  state A
  {
    a => B;
  }

  state_conc B
  {
    join j1 => A;

    state E
    {
      a => F;
    }

    state F
    {
      _ => j1;
    }

  } and
  {

    state D
    {
      a => C;
    }

    state C
    {
      _ => j1;
    }

  }

  
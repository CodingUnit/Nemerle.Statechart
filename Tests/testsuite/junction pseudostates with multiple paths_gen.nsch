

  flags testing auto_initial;
  //flags debug;
  
  name "junction pseudostates with multiple paths, see UML Reference Manual p.432 Fig.14-169";
  
  e1 => P;
  e2 => Q;
  
  junction junc1
  {
    n == 0 / c => U;
    n > 0  / d => V;
  }
  
  state P
  {
    g / a => junc1;
  }
  
  state Q
  {
    h [m > 0] / b => junc1;
  }
  
  state U
  {
  }
    
  state V
  {
  }
  
  
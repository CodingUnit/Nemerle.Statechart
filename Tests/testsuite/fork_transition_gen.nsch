

  flags testing auto_initial;
  //flags debug;

  name "fork transition";

  state A
  {
    a => C, F;
    b => B;
  }

  state_conc B
  {
    _ => A;

    state C
    {
      a => D;
    }

    state D
    {
      a => $0;
    }

  } and
  {
    state E
    {
      a => F;
    }

    state F
    {
      a => $0;
    }
  }

  

  flags = auto_initial, auto_entry_exit;
  flags testing;
  //flags debug;

  //event start_device(CanDriver);

  state NotConnected
  {
    start_device / Connect() => Connected;
  }

  state Connected
  {
    disconnect => NotConnected;

    0 => H;

    (H) => SensorImitate;

    state SensorImitate
    {
      state Imitate
      {
        file_load => FileSending;
      }

    }

    state FileSending
    {
      send_complete, file_load => SensorImitate;
    }

  }

  
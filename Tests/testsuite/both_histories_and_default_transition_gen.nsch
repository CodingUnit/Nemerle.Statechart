
  flags testing;
  //flags debug;

  name "both histories and default transition";

  state A
  {
    b => B;
    (H*) => G;
    (H)  => F;
  }

  state D : A
  {
    c => G;
    d => A.H;
    e => A.H*;
  }

  state G : A
  {
    a => D;
  }

  state E : D
  {
    a => F;
  }

  state F : D
  {
    a => C;
  }

  state B
  {
    a => A.H;
    b => A.H*;
  }

  state C
  {
    a => A;
    b => B;
  }
  
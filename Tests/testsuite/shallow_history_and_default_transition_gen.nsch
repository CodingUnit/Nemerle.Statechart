
  flags testing;
  //flags debug;
  
  name "shallow history and default transition";
  
  state A
  {
    b => B;
    (H) => G;
  }

  state D : A
  {
    c => G;
    d => A.H;
  }

  state G : A
  {
    a => D;
  }

  state E : D
  {
    a => F;
  }
      
  state F : D
  {
    a => C;
  }
      
  state B
  {
    a => A.H;
  }

  state C
  {
    a => A;
    b => B;
  }
  